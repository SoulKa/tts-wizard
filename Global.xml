<Defaults>
    <!-- scoreboard -->
    <Panel class="sb-header-entry" padding="3 3 3 3"/>
    <Text class="sb-header-entry" fontStyle="Bold" resizeTextForBestFit="true"/>
    <Panel class="sb-header-entry" color="#FFFFFF" outline="#000000" outlineSize="2 0"/>

    <Text class="sb-round-entry" fontSize="22" fontStyle="Bold" color="#000000" alignment="MiddleRight" offsetXY="-7 0"/>
    <Panel class="sb-round-entry" color="#B7B7BF"/>

    <Text class="sb-score-entry" fontSize="26" fontStyle="Bold"/>
    <Panel class="sb-score-entry" color="#FFFFFF" outline="#000000" outlineSize="2 1" minWidth="60"/>

    <Text class="sb-trick-entry" fontSize="26" fontStyle="Bold"/>
    <Panel class="sb-trick-entry" color="#FFFFFF" outline="#000000" minWidth="40"/>

    <Cell class="sb-entry" dontUseTableCellBackground="true"/>
    <Cell class="sb-empty" dontUseTableCellBackground="true"/>

    <Panel class="scoreboard" outline="#000000" outlineSize="2 2"/>

    <!-- admin panel -->
    <Panel class="admin-panel" flexibleHeight="0" outline="#000000" outlineSize="1 1"/>

    <VerticalLayout class="setting-group" childForceExpandHeight="false" />
    <ToggleGroup class="setting-group"/>
    <HorizontalLayout class="setting" preferredHeight="50" minHeight="50" height="50" flexibleHeight="0" tooltipPosition="Below" tooltipOffset="10" outline="#000000" outlineSize="1 1" color="#FFFFFF" tooltipBackgroundColor="#000000" shadow="rgba(0,0,0,0.1)" shadowDistance="-8 -8"/>
    <Panel class="setting-header" color="#B7B7BF"/>
    <Text class="setting-header" resizeTextForBestFit="true" fontStyle="Bold"/>
    <Text class="setting-name" fontSize="24"/>
</Defaults>

<!-- the admin panel -->
<VerticalLayout id="admin-panel" class="admin-panel" visibility="Admin" color="#FFFFFF" rectAlignment="MiddleRight" active="false" width="600" height="900" outline="#000000" outlineSize="2 2">
    <Panel class="admin-panel" preferredHeight="80" color="#B7B7BF" shadow="rgba(0,0,0,0.1)" shadowDistance="0 -6">
        <Text resizeTextForBestFit="true" fontStyle="Bold">{en}Preferences{de}Einstellungen</Text>
    </Panel>

    <Panel class="admin-panel" preferredHeight="820" padding="30 30 50 50">
        <VerticalLayout childAlignment="UpperCenter" childForceExpandHeight="false" spacing="40">
            <VerticalLayout class="setting-group">
                <HorizontalLayout class="setting">
                    <Panel class="setting-header">
                        <Text class="setting-header">{en}General{de}Allgemein{fr}En général</Text>
                    </Panel>
                </HorizontalLayout>
                <HorizontalLayout class="setting">
                    <Panel class="setting-name"><Text class="setting-name">{en}Mod language{de}Mod Sprache{fr}La langue du jeu</Text></Panel>
                    <Panel class="setting-input">
                        <Dropdown class="setting-input" onValueChanged="onSelectLanguage">
                            <Option class="setting-input">English</Option>
                            <Option class="setting-input">German</Option>
                            <Option class="setting-input">French</Option>
                        </Dropdown>
                    </Panel>
                </HorizontalLayout>
            </VerticalLayout>

            <ToggleGroup class="setting-group">
                <VerticalLayout class="setting-group">
                    <HorizontalLayout class="setting">
                        <Panel class="setting-header">
                            <Text class="setting-header">{en}Game variants{de}Spielvarianten{fr}Versions</Text>
                        </Panel>
                    </HorizontalLayout>
                    <HorizontalLayout class="setting" tooltip="The basic game without any variant" >
                        <Panel class="setting-name"><Text class="setting-name">{en}Classic (None){de}Standard (Keine){fr}Standard</Text></Panel>
                        <Panel class="setting-input"><Toggle id="setting-variant-none" class="setting-input" onClick="onSetGameVariant(None)"></Toggle></Panel>
                    </HorizontalLayout>
                    <HorizontalLayout class="setting" tooltip="In each round the last player that has to make the bid must place his bet such that the total number of bets made is not equal to the number of tricks available in that round. For example: If it is round 7 and the previous three players made the bets 1, 3 and 2 (1+3+2 = 6) then the last player is not allowed to bit 1 trick (round 7 and 6+1 = 7)." >
                        <Panel class="setting-name"><Text class="setting-name">{en}Plus/minus one{de}Plus/minus Eins{fr}Notequal</Text></Panel>
                        <Panel class="setting-input"><Toggle id="setting-variant-plus-minus-one" class="setting-input" onClick="onSetGameVariant(PlusMinusOne)"></Toggle></Panel>
                    </HorizontalLayout>
                    <HorizontalLayout class="setting" tooltip="Every player places his bet hidden. By that nobody is affected by the bids of the others but only by his own cards" >
                        <Panel class="setting-name"><Text class="setting-name">{en}Hidden bid{de}Versteckter Tipp{fr}Pli cachée</Text></Panel>
                        <Panel class="setting-input"><Toggle id="setting-variant-hidden-bid" class="setting-input" onClick="onSetGameVariant(HiddenBid)"></Toggle></Panel>
                    </HorizontalLayout>
                </VerticalLayout>
            </ToggleGroup>
        </VerticalLayout>
    </Panel>
</VerticalLayout>

